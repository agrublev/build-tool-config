{
  "name": "@milesj/build-tool-config",
  "version": "0.89.0",
  "description": "Build tool configuration files for easy re-use.",
  "keywords": [
    "build",
    "tool",
    "config",
    "babel",
    "eslint",
    "jest",
    "prettier",
    "typescript",
    "webpack"
  ],
  "scripts": {
    "eslint": "beemo eslint .",
    "prettier": "beemo prettier \"./{configs,dotfiles,scripts}/*.{js,json,md}\"",
    "test": "yarn run eslint",
    "release": "np --no-cleanup --no-yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/build-tool-config.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.5.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@beemo/cli": "^0.16.0",
    "@beemo/core": "^0.16.0",
    "@beemo/driver-babel": "^0.16.0",
    "@beemo/driver-eslint": "^0.16.0",
    "@beemo/driver-jest": "^0.16.0",
    "@beemo/driver-prettier": "^0.16.0",
    "@beemo/driver-typescript": "^0.5.0",
    "@types/jest": "^22.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-dev": "^2.0.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-typescript": "^0.12.0",
    "eslint-plugin-unicorn": "^4.0.3",
    "execa": "^0.10.0",
    "fs-extra": "^6.0.0",
    "gitbook-cli": "^2.3.2",
    "jest": "^22.4.3",
    "jest-runner-eslint": "^0.5.0",
    "np": "^2.20.1",
    "prettier": "^1.12.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "^4.7.0"
  },
  "beemo": {
    "module": "@local",
    "drivers": [
      "eslint",
      "prettier"
    ],
    "eslint": {
      "rules": {
        "no-var": "off",
        "no-console": "off",
        "promise/always-return": "off"
      }
    }
  }
}
